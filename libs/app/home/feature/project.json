{
  "name": "app-home-feature",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/app/home/feature/src",
  "prefix": "app",
  "tags": [],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/app/home/feature/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/app/home/feature/**/*.ts",
          "libs/app/home/feature/**/*.html"
        ]
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "libs/app/home/feature/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "app:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "app:serve:production"
        },
        "ci": {
          "devServerTarget": "app:serve-static"
        }
      }
    }
  }
}
