{
  "extends": ["../../../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "@typescript-eslint/no-explicit-any": [
          "off",
          {
            "ignoreRestArgs": true
          }
        ]
      },
      "extends": [
        "plugin:@typescript-eslint/recommended"
      ]
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
